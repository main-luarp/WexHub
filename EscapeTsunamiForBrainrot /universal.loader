-- Smooth Load
local function simulateLoading(duration)
	local elapsed = 0
	while elapsed < duration do
		task.wait(0.05)
		elapsed += 0.05
		local ratio = elapsed / duration
		local eased = 1 - math.exp(-3 * ratio)
		BarFill.Size = UDim2.new(eased, 0, 1, 0)
		Percent.Text = math.floor(eased * 100) .. "%"
	end
end

simulateLoading(500) -- duration in seconds

