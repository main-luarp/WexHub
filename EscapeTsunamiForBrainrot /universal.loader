-- üå† Shooting Stars
local function createStar()
	local star = Instance.new("Frame")
	local size = math.random(3, 6)
	star.Size = UDim2.new(0, size, 0, size)
	star.Position = UDim2.new(math.random(), 0, math.random(), 0)
	star.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	star.BorderSizePixel = 0
	star.ZIndex = 1 -- ‚¨ÖÔ∏è lowered so stars stay behind text
	star.Parent = Background

	local trail = Instance.new("ImageLabel")
	trail.Size = UDim2.new(0, size * 20, 0, size * 2)
	trail.AnchorPoint = Vector2.new(1, 0.5)
	trail.Position = UDim2.new(0, 0, 0.5, 0)
	trail.BackgroundTransparency = 1
	trail.Image = "rbxassetid://4964674313"
	trail.ImageColor3 = Color3.fromRGB(120, 180, 255)
	trail.ImageTransparency = 0.25
	trail.ZIndex = 1 -- ‚¨ÖÔ∏è same reason
	trail.Parent = star

	local endPos = UDim2.new(star.Position.X.Scale + 0.4, 0, star.Position.Y.Scale + 0.1, 0)
	local tween = TweenService:Create(star, TweenInfo.new(math.random(1, 2), Enum.EasingStyle.Linear), {
		Position = endPos,
		BackgroundTransparency = 1
	})
	local trailTween = TweenService:Create(trail, TweenInfo.new(1.2), {ImageTransparency = 1})
	tween:Play()
	trailTween:Play()
	tween.Completed:Connect(function()
		star:Destroy()
	end)
end

task.spawn(function()
	while task.wait(math.random(0.3, 0.7)) do
		createStar()
	end
end)

-- üåà Premium Nebula Gradient (used everywhere)
local function premiumGradient(parent)
	local g = Instance.new("UIGradient")
	g.Color = ColorSequence.new({
		ColorSequenceKeypoint.new(0, Color3.fromRGB(170, 100, 255)), -- Violet
		ColorSequenceKeypoint.new(0.5, Color3.fromRGB(80, 150, 255)), -- Blue
		ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 255, 200)) -- Aqua
	})
	g.Rotation = 0
	g.Parent = parent
	task.spawn(function()
		while task.wait(0.05) do
			g.Rotation = (g.Rotation + 1) % 360
		end
	end)
	return g
end

